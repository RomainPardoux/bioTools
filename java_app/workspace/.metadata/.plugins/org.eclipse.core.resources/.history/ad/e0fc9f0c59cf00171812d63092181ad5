package com.sequence;

public class Nucleotid {

	// Variable d'instance
	private char nucleotid;
	
	// Constructeur
	public Nucleotid(char nucleotid) {
		this.nucleotid = nucleotid;
	}

	// Getters and Setters
	public char getNucleotid() {
		return nucleotid;
	}

	public void setNucleotid(char nucleotid) {
		this.nucleotid = nucleotid;
	}

	
	
}
